{
  "manifest_version": 2,

  "name": "Chatension",
  "description": "Chat on any url",
  "version": "1.0.2",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "chatensionLogo.png",
    "default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "http://*/*", 
    "https://*/*"
  ]
}